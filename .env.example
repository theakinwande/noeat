# Example environment configuration file
# Copy this file to .env and fill in your actual API keys

# Google Maps API Configuration
# Get your API key from: https://console.cloud.google.com/
# Required APIs: Maps JavaScript API, Places API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Alternative API Services (optional)
# VITE_YELP_API_KEY=your_yelp_fusion_api_key_here
# VITE_FOURSQUARE_API_KEY=your_foursquare_places_api_key_here
# VITE_TOMTOM_API_KEY=your_tomtom_maps_api_key_here
# VITE_HERE_API_KEY=your_here_maps_api_key_here
# VITE_MAPBOX_API_KEY=your_mapbox_api_key_here

# Application Configuration
VITE_APP_NAME=NoEat
VITE_APP_ENV=development

# Backend API (if you add one in the future)
# VITE_API_BASE_URL=http://localhost:3000/api